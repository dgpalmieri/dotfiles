# Environment Variables

export EDITOR=/bin/nvim

## gpg interface
    export GPG_TTY=$(tty)

## Starship config
    export STARSHIP_CONFIG=/home/dgpalmieri/.config/starship/starship.toml

## Add local bin and cargo bin to my $PATH
    export PATH=$PATH:/home/dgpalmieri/.local/bin
    export PATH=$PATH:/home/dgpalmieri/.cargo/bin

## Add completions to $fpath
    export fpath=(~/.zsh-plugins/completions ~/.zsh_plugins/zsh-completions/src $fpath)

